<?xml version="1.0" encoding="UTF-8"?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bonplan.TypeBonPlanSearchByLocationController">
      <stylesheets>
        <URL value="@cass.css" />
    </stylesheets>
    <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="31.0" prefWidth="81.0" text="Location: ">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtFieldLocation" layoutX="102.0" layoutY="14.0" prefHeight="31.0" prefWidth="166.0" />
      <Button fx:id="btnSearchLocation" layoutX="300.0" layoutY="14.0" mnemonicParsing="false" onAction="#seachLocation" prefHeight="32.0" prefWidth="81.0" text="Search">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Button>
      <ListView fx:id="resultList" layoutX="14.0" layoutY="67.0" prefHeight="304.0" prefWidth="496.0" />
      <Button fx:id="btnReturnToHome" layoutX="421.0" layoutY="14.0" mnemonicParsing="false" onAction="#returnToHome" prefHeight="32.0" prefWidth="166.0" text="Return To Home">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
